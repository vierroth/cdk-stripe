{
	"name": "@flit/cdk-stripe",
	"version": "0.0.11",
	"description": "A collection of AWS CDK constructs to manager your Stripe resources programmatically",
	"keywords": [
		"aws",
		"cdk",
		"awscdk",
		"aws-cdk",
		"stripe"
	],
	"homepage": "https://github.com/vierroth/cdk-stripe",
	"bugs": "https://github.com/vierroth/cdk-stripe/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/vierroth/cdk-stripe"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Vierroth GmbH",
		"url": "https://github.com/vierroth"
	},
	"contributors": [
		{
			"name": "Luis Vierroth",
			"url": "https://github.com/p-mercury"
		}
	],
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"./dist",
		"./src",
		"./.jsii",
		"./LICENSE.txt",
		"./README.md"
	],
	"scripts": {
		"build": "jsii",
		"prepack": "jsii"
	},
	"prettier": {
		"plugins": [
			"prettier-plugin-packagejson"
		],
		"useTabs": true
	},
	"devDependencies": {
		"@aws-cdk/aws-lambda-go-alpha": "^2.197.0-alpha.0",
		"@aws-sdk/client-secrets-manager": "^3.812.0",
		"@types/aws-lambda": "^8.10.149",
		"@types/node": "^22.15.21",
		"aws-cdk-lib": "^2.197.0",
		"constructs": "^10.4.2",
		"jest": "^29.7.0",
		"jsii": "^5.8.9",
		"jsii-pacmak": "^1.112.0",
		"prettier": "^3.5.3",
		"prettier-plugin-packagejson": "^2.5.14",
		"ts-jest": "^29.3.4",
		"typescript": "^5.8.3"
	},
	"peerDependencies": {
		"aws-cdk-lib": "^2.197.0",
		"constructs": "^10.4.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"jsii": {
		"outdir": "./dist",
		"excludeTypescript": [
			"**/handler.ts"
		],
		"tsc": {
			"rootDir": "./src",
			"outDir": "./dist"
		},
		"targets": {
			"java": {
				"package": "flit.cdk-stripe",
				"maven": {
					"groupId": "flit",
					"artifactId": "cdk-stripe"
				}
			},
			"dotnet": {
				"namespace": "Flit.CDK.Stripe",
				"packageId": "Flit.CDK.Stripe"
			},
			"python": {
				"libName": "flit.cdk-stripe",
				"module": "flit.cdk-stripe",
				"classifiers": [
					"Framework :: AWS CDK",
					"Framework :: AWS CDK :: 2"
				]
			}
		}
	},
	"stability": "stable"
}
